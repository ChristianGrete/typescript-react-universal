{
  "name": "typescript-react-universal",
  "version": "1.0.0",
  "description": "TypeScript React Universal",
  "main": "index.js",
  "author": {
    "name": "Reed Dadoune",
    "email": "reed@dadoune.com",
    "url": "https://www.dadoune.com"
  },
  "license": "MIT",
  "scripts": {
    "start": "./dev-server",
    "build": "npm run ts:lint && rm -rf ./dist && npm run webpack:build",
    "precommit": "lint-staged",
    "prettier": "yarn run prettier:ts && yarn run prettier:js",
    "prettier:ts": "prettier --single-quote --trailing-comma all --write \"./{src,webpack}/**/*.{ts,tsx}\"",
    "prettier:js": "prettier --single-quote --trailing-comma es5 --write \"./{src,webpack}/**/*.{js,jsx}\" dev-server",
    "ts:lint": "tslint -c tslint.json -p tsconfig.json",
    "webpack:build": "webpack --config ./webpack/webpack.config.js"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --single-quote --trailing-comma all --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "dependencies": {
    "@types/koa-compose": "^3.2.2",
    "axios": "^0.16.2",
    "bcrypt": "^1.0.2",
    "bluebird": "^3.5.0",
    "clean-css": "^4.1.3",
    "consolidate": "^0.14.5",
    "history": "^4.6.1",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compose": "^4.0.0",
    "koa-compress": "^2.0.0",
    "koa-html-minifier": "^1.0.1",
    "koa-router": "^7.2.1",
    "koa-static": "^4.0.1",
    "koa-views": "^6.0.2",
    "lodash": "^4.17.4",
    "material-ui": "1.0.0-beta.6",
    "mongoose": "^4.10.5",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.0",
    "pug": "^2.0.0-rc.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-jss": "^7.1.0",
    "react-router-dom": "^4.1.1",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "satcheljs": "3.0.0-beta5"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/bluebird": "^3.5.5",
    "@types/clean-css": "^3.4.30",
    "@types/koa": "^2.0.39",
    "@types/koa-bodyparser": "^3.0.24",
    "@types/koa-compress": "^2.0.7",
    "@types/koa-router": "^7.0.23",
    "@types/koa-static": "^3.0.1",
    "@types/koa-views": "^2.0.2",
    "@types/lodash": "^4.14.66",
    "@types/mongodb": "^2.2.4",
    "@types/mongoose": "^4.7.17",
    "@types/passport": "^0.3.3",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.0",
    "@types/react-router": "^4.0.11",
    "@types/react-router-dom": "^4.0.4",
    "@types/react-tap-event-plugin": "^0.0.30",
    "awesome-typescript-loader": "^3.1.3",
    "babel-core": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.18.12",
    "copy-webpack-plugin": "^4.0.1",
    "gulp-nodemon": "^2.2.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.4",
    "nodemon": "^1.11.0",
    "prettier": "^1.4.4",
    "tslint": "^5.4.2",
    "tslint-react": "^3.0.0",
    "typescript": "^2.4.2",
    "webpack": "^3.5.5",
    "webpack-node-externals": "^1.6.0"
  }
}
