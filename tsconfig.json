{
	"compilerOptions": {
		"module": "es6",
		"target": "es6",
		"jsx": "react",
		"noImplicitAny": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "dist/server",
		"baseUrl": ".",
		"experimentalDecorators": true,
		"paths": {
			"*": [
				"node_modules/*",
				"src/common/*",
				"src/types/*",
				"src/*"
			]
		}
	},
	"include": [
		"src/**/*"
	]
}
